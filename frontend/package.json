{
  "name": "react-web-ui",
  "version": "0.1.0",
  "description": "React web UI for various applications",
  "author": "KFrost",
  "license": "MIT",
  "private": true,
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=6.0.0"
  },
  "dependencies": {
    "@chakra-ui/react": "^3.30.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "framer-motion": "^12.23.25",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-router-dom": "^7.10.1",
    "react-scripts": "^5.0.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "format": "prettier --write ."
  },
  "devDependencies": {
    "eslint": "^8.45.0",
    "eslint-config-react-app": "^7.0.1",
    "prettier": "^3.0.0",
    "vite": "^4.4.9",
    "typescript": "^5.2.2"
  },
  "eslintConfig": { 
    "extends": ["react-app", "eslint:recommended"], 
    "rules": { 
      "no-unused-vars": "warn", 
      "no-console": "off" 
    } 
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
